CREATE TABLE `riskfactor`(
  `driverid` string, 
  `events` bigint, 
  `totmiles` bigint, 
  `riskfactor` float)
ROW FORMAT SERDE 
  'org.apache.hadoop.hive.ql.io.orc.OrcSerde' 
STORED AS INPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.orc.OrcInputFormat' 
OUTPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.orc.OrcOutputFormat'
LOCATION
  'hdfs://onegovdev/apps/hive/warehouse/test.db/riskfactor'
TBLPROPERTIES (
  'numFiles'='1', 
  'numRows'='0', 
  'rawDataSize'='0', 
  'totalSize'='1468', 
  'transient_lastDdlTime'='1492642694')
