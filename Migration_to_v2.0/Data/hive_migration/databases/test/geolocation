CREATE TABLE `geolocation`(
  `truckid` string, 
  `driverid` string, 
  `event` string, 
  `latitude` double, 
  `longitude` double, 
  `city` string, 
  `state` string, 
  `velocity` int, 
  `event_ind` int, 
  `idling_ind` int)
ROW FORMAT SERDE 
  'org.apache.hadoop.hive.ql.io.orc.OrcSerde' 
STORED AS INPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.orc.OrcInputFormat' 
OUTPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.orc.OrcOutputFormat'
LOCATION
  'hdfs://onegovdev/apps/hive/warehouse/test.db/geolocation'
TBLPROPERTIES (
  'COLUMN_STATS_ACCURATE'='{\"BASIC_STATS\":\"true\"}', 
  'numFiles'='1', 
  'numRows'='8000', 
  'rawDataSize'='3808000', 
  'totalSize'='52810', 
  'transient_lastDdlTime'='1492582797')
