CREATE TABLE `crn_viz`(
  `weekday` string, 
  `depth` bigint, 
  `layer` string, 
  `contoursequence` string, 
  `train_date` string, 
  `locationid` string, 
  `latitude` double, 
  `longitude` double, 
  `markid` string, 
  `tripid` string, 
  `numberofrecords` string, 
  `operatorcode` string, 
  `signalid` string, 
  `signalstrength` string, 
  `commoditydescription` string)
ROW FORMAT SERDE 
  'org.apache.hadoop.hive.serde2.lazy.LazySimpleSerDe' 
STORED AS INPUTFORMAT 
  'org.apache.hadoop.mapred.TextInputFormat' 
OUTPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.HiveIgnoreKeyTextOutputFormat'
LOCATION
  'hdfs://onegovdev/apps/hive/warehouse/4_02_freight_analytics.db/crn_viz'
TBLPROPERTIES (
  'COLUMN_STATS_ACCURATE'='{\"BASIC_STATS\":\"true\"}', 
  'numFiles'='1', 
  'numRows'='45746', 
  'rawDataSize'='4891840', 
  'totalSize'='4937586', 
  'transient_lastDdlTime'='1504672702')
