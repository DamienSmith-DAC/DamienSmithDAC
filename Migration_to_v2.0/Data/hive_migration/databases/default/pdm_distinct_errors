  'COLUMN_STATS_ACCURATE'='{\"BASIC_STATS\":\"true\"}', 
  'numFiles'='1', 
  'numRows'='6', 
  'rawDataSize'='37', 
  'totalSize'='43', 
  'transient_lastDdlTime'='1500182947')
