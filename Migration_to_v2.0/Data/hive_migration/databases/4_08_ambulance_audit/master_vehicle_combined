CREATE TABLE `master_vehicle_combined`(
  `response_area` string, 
  `geo_sector` string, 
  `priority_category` string, 
  `call_disposition` string, 
  `year` string, 
  `rva_pk` string, 
  `rva_id` string, 
  `rva_master_incident_id` string, 
  `battalion` string, 
  `hour_arrivedatscene` string, 
  `hour_assigned` string, 
  `vehicle_id` string, 
  `cancel_reason` string, 
  `vehicle_type` string)
